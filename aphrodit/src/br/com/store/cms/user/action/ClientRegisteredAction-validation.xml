<!DOCTYPE validators PUBLIC "-//Apache Struts//XWork Validator 1.0.3//EN"
		"http://struts.apache.org/dtds/xwork-validator-1.0.3.dtd">
<validators>

	<field name="email">
		<field-validator type="required">
			<param name="trim">true</param>
			<message key="errors.required" />
		</field-validator>
		<field-validator type="email">
			<message key="errors.invalid" />
		</field-validator>
	</field>
	<field name="password">
		<field-validator type="required">
			<param name="trim">true</param>
			<message key="errors.required" />
		</field-validator>
	</field>
	<field name="retypedPassword">
		<field-validator type="required">
			<param name="trim">true</param>
			<message key="errors.required" />
		</field-validator>
	</field>
	<field name="name">
		<field-validator type="required">
			<param name="trim">true</param>
			<message key="errors.required" />
		</field-validator>
	</field>
	<field name="nickName">
		<field-validator type="required">
			<param name="trim">true</param>
			<message key="errors.required" />
		</field-validator>
	</field>
	<field name="rg">
		<field-validator type="required">
			<param name="trim">true</param>
			<message key="errors.required" />
		</field-validator>
	</field>
	<field name="birthday">
		<field-validator type="required">
			<param name="trim">true</param>
			<message key="errors.required" />
		</field-validator>
		<field-validator type="date">
			<message key="errors.invalid" />
		</field-validator>
    </field>
	<field name="optIn">
		<field-validator type="required">
			<param name="trim">true</param>
			<message key="errors.required" />
		</field-validator>
    </field>
</validators>